                          <h1>Inschrijfformulier</h1>
                         <form action="https://formspree.io/cwstam84@gmail.com" method="post">
					<table style="width: 500px;
					border-spacing: 0px; padding: 0px ">
						<tr><td>&nbsp;</td></tr>
						<tr>
							<td style=" width: 250px;
							text-align: right">Naam:&nbsp;</td>
							<td>
							<input type="text" name="naam" size="40" maxlength="40"></td>
						</tr>
						<tr>
							<td style=" width: 250px;
							text-align: right">Straat en huisnummer:&nbsp;</td>
							<td>
							<input type="text" name="straat en huisnummer" size="40" maxlength="40"></td>
						</tr>
						<tr>
							<td style=" width: 250px;
							text-align: right">Postcode en woonplaats:&nbsp;</td>
							<td>
							<input type="text" name="postcode en woonplaats" size="40" maxlength="40"></td>
						</tr>
						<tr>
							<td style=" width: 250px;
							text-align: right">E-mailadres:&nbsp;</td>
							<td>
							<input type="email" name="email" size="40" maxlength="40" required></td>
						</tr>
						<tr>
							<td style=" width: 250px;
							text-align: right">Betaalmethode:&nbsp;</td>
							<td>
								<input type="radio" value="rembourszending" name="kaart" checked>Rembourszending
								<input type="radio" value="vooruitbetaling" name="kaart">Vooruitbetaling
							</td>
						</tr>						
						<tr>
							<td style=" width: 250px;
							text-align: right">Soort:&nbsp;</td>
							<td>
								<input type="checkbox" value="check" name="apparaten">Apparaten
								<input type="checkbox" value="check" name="tuinafval">Tuinafval
								<input type="checkbox" value="check" name="grofafval">Grof afval
							</td>
						</tr>						
						<tr>
							<td style=" width: 250px;
							text-align: right">Apparaat:&nbsp;</td>
							<td>
								<select class="alle_rij" name="product" size="2" multiple="">
									<option class="eerste_rij">Kies elektrische apparaten</option>
									<option>Afzuigkap</option>
									<option>Computerapparaat</option>
									<option>Koelkast</option>
									<option>Koffiezetapparaat</option>
									<option>Radio</option>
									<option>Televisie</option>
									<option>Vaatwasser</option>
									<option>Wasmachine</option>
								</select>
							</td>
						</tr>	
						<tr>
							<td style=" width: 250px;
							text-align: right">Tuinafval:&nbsp;</td>
							<td>
								<select class="alle_rij" name="product" size="2" multiple="">
									<option class="eerste_rij">Kies tuinafval</option>
									<option>Groenteafval</option>
									<option>Stammetjes en stronken dikker dan 10cm</option>
									<option>Stammetjes en stronken dunner dan 10cm</option>
									<option>Tuinafval</option>
								</select>
							</td>
						</tr>				
						<tr>
							<td style=" width: 250px;
							text-align: right">Grof afval:&nbsp;</td>
							<td>
								<select class="alle_rij" name="product" size="2" multiple="">
									<option class="eerste_rij">Kies grof afval</option>
									<option>Bank</option>
									<option>Bureau - hout</option>
									<option>Bureau - staal</option>
									<option>Kachel</option>
									<option>Laminaat</option>
									<option>Trap/ladder - hout</option>
									<option>Vloerbedekking</option>
									<option>Wandbekleding</option>
									<option>Zonwering - hout</option>
									<option>Zonwering - metaal</option>
								</select>
							</td>
						</tr>						
						<tr>
							<td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
							<td><input type="submit" value="Verzenden">
							<input type="reset" value="Wissen"></td>
						</tr>
					</table>
				</form>
